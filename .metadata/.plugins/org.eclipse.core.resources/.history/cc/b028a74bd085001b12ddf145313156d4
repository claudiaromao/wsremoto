package variaveis;

public class TipoString {

	public static void main(String[] args) {
		String email= "Claudia.Romao@itau-Unibanco.Com.Br";
		System.out.println("Original:" + email);
		System.out.println("Minúscula:" + email.toLowerCase());
		System.out.println("Maiúscula:" + email.toUpperCase());
		System.out.println("Qtde Caracteres:" + email.length());
		System.out.println("É igual:" + email.equals("c@itau.com.br"));
		System.out.println("É igual 2:" + email.equalsIgnoreCase("c@itau.com.br"));
		System.out.println("Tem arroba?" + email.contains("@"));
		
	
		
		
		
		
		
	}
	
	
}
